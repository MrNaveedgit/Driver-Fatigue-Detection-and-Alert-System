<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.artemis.dfds_app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.artemis.dfds_app.MainActivity" />
        <activity
            android:name="com.example.artemis.dfds_app.ContactActivity"
            android:label="@string/title_activity_contact"
            android:parentActivityName="com.example.artemis.dfds_app.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.artemis.dfds_app.MainActivity" />
        </activity>
        <activity
            android:name="com.example.artemis.dfds_app.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.example.artemis.dfds_app.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.artemis.dfds_app.MainActivity" />
        </activity>
        <activity android:name="com.example.artemis.dfds_app.WelcomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.artemis.dfds_app.GuideActivity"
            android:label="@string/title_guide_activity"
            android:parentActivityName="com.example.artemis.dfds_app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.artemis.dfds_app.MainActivity" />
        </activity>
        <activity android:name="com.example.artemis.dfds_app.GuideWelcomeActivity" />
    </application>

</manifest>